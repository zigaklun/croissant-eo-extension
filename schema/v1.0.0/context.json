{
  "@context": {
    "eo": "https://zigaklun.github.io/croissant-eo-extension/schema/v1.0.0/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",

    "eo:scope": {
      "@type": "xsd:string",
      "@id": "eo:scope"
    },
    "eo:has-task": {
      "@type": "@json",
      "@id": "eo:has-task"
    },
    "eo:amountOfTrainingData": {
      "@type": "xsd:string",
      "@id": "eo:amountOfTrainingData"
    },
    "eo:numberOfClasses": {
      "@type": "xsd:string",
      "@id": "eo:numberOfClasses"
    },
    "eo:band": {
      "@type": "@json",
      "@container": "@set",
      "@id": "eo:band"
    },
    "eo:has_class": {
      "@type": "@json",
      "@container": "@set",
      "@id": "eo:has_class"
    },
    "eo:statisticsInfo": {
      "@type": "@json",
      "@container": "@set",
      "@id": "eo:statisticsInfo"
    },
    "eo:metricsInLIT": {
      "@type": "@json",
      "@container": "@set",
      "@id": "eo:metricsInLIT"
    },
    "eo:labeling": {
      "@type": "@json",
      "@id": "eo:labeling"
    },
    "eo:spatialResolutionInMeters": {
      "@type": "xsd:string",
      "@id": "eo:spatialResolutionInMeters"
    },
    "eo:status": {
      "@type": "xsd:string",
      "@id": "eo:status"
    }
  }
}
