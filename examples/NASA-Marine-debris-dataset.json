{
  "@context": {
    "cr": "https://mlcommons.org/croissant/",
    "sc": "https://schema.org/",
    "eo": "https://zigaklun.github.io/croissant-eo-extension/schema/v1.0.0/"
  },
  "@type": "sc:Dataset",
  "name": "marine-debris-detection",
  "description": "Dataset for marine debris detection from satellite imagery",
  "version": "1.0.0",
  "license": "MIT",

  "eo:scope": "Images were acquired over coastal regions of Honduras, Greece and Ghana.",
  "eo:has-task": {
    "description": "Object detection with a bounding box around marine debris.",
    "taskType": "object detection"
  },
  "eo:amountOfTrainingData": "739",
  "eo:numberOfClasses": "2",
  "eo:band": [
    {
      "label": "B1",
      "value": "Red (600-700nm)"
    },
    {
      "label": "B2",
      "value": "Green (500-600nm)"
    },
    {
      "label": "B3",
      "value": "Blue (400-450nm)"
    }
  ],
  "eo:has_class": [
    {
      "label": "marine debris",
      "identifier": 1
    },
    {
      "label": "empty",
      "identifier": 2
    }
  ],
  "eo:statisticsInfo": [
    {
      "label": "number of image patches",
      "value": "739"
    },
    {
      "label": "number of .jpg image patches",
      "value": "739"
    },
    {
      "label": "number of .tif image patches",
      "value": "739"
    },
    {
      "label": "number of marine debris",
      "value": "739"
    }
  ],
  "eo:metricsInLIT": [
    {
      "identifier": "https://github.com/NASA-IMPACT/marine_debris_ML",
      "algorithm": "SSD Resnet 101 Feature Pyramid Network (FPN)",
      "literature_metric": [
        {
          "label": "Average recall",
          "value": "0.70"
        },
        {
          "label": "F1",
          "value": "0.74"
        }
      ]
    }
  ],
  "eo:labeling": {
    "identifier": "nasa-marine-debries-labeling",
    "labeler": [
      {
        "identifier": "nasa-marine-debries-labeling_1",
        "name": "Shaia, A."
      },
      {
        "identifier": "nasa-marine-debries-labeling_2",
        "name": "Thomas, L."
      },
      {
        "identifier": "nasa-marine-debries-labeling_3",
        "name": "Maskey, M."
      }
    ],
    "procedure": {
      "identifier": "nasa-marine-debries-labeling-procedure",
      "scope": "Annotate each building instance with a bounding box and damage mask across video frames.",
      "method": [
        {
          "label": "labels",
          "definition": "Image Labeler used to manually digitize bounding box annotations for observable debris on Planetscope optical imagery."
        }
      ]
    }
  },
  "eo:spatialResolutionInMeters": "10",
  "eo:status": "completed",

  "distribution": [
    {
      "@type": "cr:FileSet",
      "@id": "image_source_jpg_set",
      "name": "image_source_jpg_set",
      "description": "Image files",
      "encodingFormat": "image/jpeg",
      "includes": "NASAMarineDebris/source/201*.jpg"
    },
    {
      "@type": "cr:FileSet",
      "@id": "image_source_tif_set",
      "name": "image_source_tif_set",
      "description": "Image files",
      "encodingFormat": "image/tiff",
      "includes": "NASAMarineDebris/source/201*.tif"
    },
    {
      "@type": "cr:FileSet",
      "@id": "mask_labels_geojson_set",
      "name": "mask_labels_geojson_set",
      "description": "Mask files",
      "encodingFormat": "application/json",
      "includes": "NASAMarineDebris/labels/201*.geojson"
    },
    {
      "@type": "cr:FileSet",
      "@id": "mask_labels_npy_set",
      "name": "mask_labels_npy_set",
      "description": "Mask files",
      "encodingFormat": "application/npy",
      "includes": "NASAMarineDebris/labels/201*.npy"
    }
  ],
  "recordSet": [
    {
      "@type": "cr:RecordSet",
      "@id": "images",
      "name": "Images",
      "description": "Collection of image data",
      "key": {
        "@id": "image_source_jpg_id"
      },
      "field": [
        {
          "@type": "cr:Field",
          "@id": "image_source_jpg_id",
          "name": "image_source_jpg",
          "dataType": "sc:ImageObject",
          "source": {
            "fileSet": {
              "@id": "image_source_jpg_set"
            },
            "extract": {
              "fileProperty": "content"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "image_source_tif_id",
          "name": "image_source_tif",
          "dataType": "sc:ImageObject",
          "source": {
            "fileSet": {
              "@id": "image_source_tif_set"
            },
            "extract": {
              "fileProperty": "content"
            }
          }
        }
      ]
    },
    {
      "@type": "cr:RecordSet",
      "@id": "labels",
      "name": "Labels/Annotations",
      "description": "Collection of bounding box annotations for object detection",
      "key": {
        "@id": "mask_labels_geojson_id"
      },
      "field": [
        {
          "@type": "cr:Field",
          "@id": "mask_labels_geojson_id",
          "name": "mask_labels_geojson",
          "dataType": "cr:BoundingBox",
          "source": {
            "fileSet": {
              "@id": "mask_labels_geojson_set"
            },
            "extract": {
              "fileProperty": "content"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "mask_labels_npy_id",
          "name": "mask_labels_npy",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "mask_labels_npy_set"
            },
            "extract": {
              "fileProperty": "content"
            }
          }
        }
      ]
    }
  ]
}
